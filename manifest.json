{
  "name": "LogMaster",
  "version": "0.0.1",
  "description": "LogMaster",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://ssl.google-analytics.com; object-src 'self'",
  "permissions": [
    "tabs",
    "storage",
    "bookmarks",
    "contextMenus"
  ],
  "omnibox": {
    "keyword": "LogMaster"
  },
  "options_page": "html/main.html",
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_title": "LogMaster"
  }
}